{
  "name": "mobile",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start --dev-client",
    "start:tunnel": "expo start --tunnel --dev-client",
    "start:clear": "expo start --clear --dev-client",
    "android": "expo start --android --dev-client",
    "ios": "expo start --ios --dev-client",
    "web": "expo start --web",
    "build:dev": "eas build -p android --profile development",
    "build:apk": "eas build -p android --profile preview",
    "build:aab": "eas build -p android --profile production"
  },
  "dependencies": {
    "@react-navigation/native": "^7.1.25",
    "@react-navigation/native-stack": "^7.8.6",
    "axios": "^1.13.2",
    "expo": "~54.0.29",
    "expo-dev-client": "~6.0.20",
    "expo-local-authentication": "^17.0.8",
    "expo-secure-store": "^15.0.8",
    "expo-status-bar": "~3.0.9",
    "react": "19.1.0",
    "react-native": "0.81.5",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0"
  },
  "devDependencies": {
    "@types/react": "~19.1.0",
    "sharp": "^0.34.5",
    "typescript": "~5.9.2"
  },
  "private": true
}
